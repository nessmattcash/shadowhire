{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shadowhire": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shadowhire",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "zone.js",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/strategy/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/strategy/vendor/aos/aos.css",
              "src/assets/strategy/vendor/glightbox/css/glightbox.min.css",
              "src/assets/strategy/vendor/swiper/swiper-bundle.min.css",
              "src/styles.scss",
              "src/assets/strategy/css/main.css",
                "node_modules/swiper/swiper-bundle.min.css",

              "src/assets/admin/css/main.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/strategy/vendor/aos/aos.js",
              "src/assets/strategy/vendor/glightbox/js/glightbox.min.js",
              "src/assets/strategy/vendor/isotope-layout/isotope.pkgd.min.js",
              "src/assets/strategy/vendor/swiper/swiper-bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "publicHost": "http://localhost:4200",
            "allowedHosts": ["all"],
            "servePath": "/"
          },
          "configurations": {
            "production": {
              "buildTarget": "shadowhire:build:production"
            },
            "development": {
              "hmr": true,
              "buildTarget": "shadowhire:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.scss",
                "node_modules/swiper/swiper-bundle.min.css"

            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "35211335-25a4-4701-8181-34bfa80d0b2d"
  }
}